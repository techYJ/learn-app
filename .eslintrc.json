{
    "env": {
        "browser": true,
        "es2021": true
    },
    "parser": "@babel/eslint-parser",
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "plugin:react/jsx-runtime"
    ],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "babelOptions": {
            "presets": ["@babel/preset-react"]
        }
    },
    "plugins": [
        "react",
        "@babel"
    ],
    "rules": {
        "react/prop-types": "off",
        "react/no-unescaped-entities": "off"
    },
    "settings": {
        "react": {
            "version": "detect"
        }
    }
}